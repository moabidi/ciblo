<div class="result-search container search-table hide">
    {#<div class="search-box">#}
        {#<div class="row">#}
            {#<div class="col-md-3">#}
                {#<h5>50 Records Found</h5>#}
            {#</div>#}
            {#<div class="col-md-9">#}
                {#<input type="text" id="inputStat" class="form-control" placeholder="Search all fields e.g. HTML">#}
            {#</div>#}
        {#</div>#}
    {#</div>#}
    {#<div class="card card-inverse card-danger text-xs-center">#}

        <div class="search-list table-responsive table-wrapper-scroll-y table-fixed">
            <table class="table" id="resultSearch">
                {% if globalResult|length %}
                    <thead>
                    {% for fieldName, value in globalResult.0 %}
                        <th>{{ fieldName|trans }}</th>
                    {% endfor %}
                    </thead>
                    <tbody>
                    {% for row in globalResult %}
                        <tr data-country="{{ row.countryCode|default('') }}">
                            {% for fieldName, value in row %}
                                <td>{{ value|raw }}</td>
                            {% endfor %}
                        </tr>
                    {% endfor %}
                    </tbody>
                {% endif %}
            </table>
        </div>
    {% include 'OivBundle:blocks:pagination.html.twig' %}
    {#{% else %}#}
            {#<div class="alert alert-warning alert-dismissible" role="alert">#}
                {#<button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">&times;</span></button>#}
                {#<strong>Aucun r√©sultat !</strong> Veuillez refaire une nouvelle recherche.#}
            {#</div>#}
        {#{% endif %}#}
    {#</div>#}
    <button id="btn-global-search" class="hide" name="searchData"></button>
</div>