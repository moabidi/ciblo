<div id="comp-menu" class="col-lg-12 col-md-12 col-sm-12 col-xs-12 hidden-xs">
    <div class="col-lg-3 col-md-3 col-sm-3 col-xs-9"></div>
    <div class="col-lg-9 col-md-9 col-sm-9 col-xs-12 cont">
        <h2 class="demo-name"><a href="https://github.com/moabidi/ciblo">Ciblo Test Demos</a> &rsaquo; Abidi Mohamed</h2>
        {% if app.request.attributes.get('_route') in ['home','device','os','navigator'] %}
        <div class="btn-group theme">
            <a class="btn btn-theme {% if app.request.attributes.get('_route') =='home' %}disabled{% endif %}" href="/">Résolution</a>
            <a class="btn btn-theme {% if app.request.attributes.get('_route') =='device' %}disabled{% endif %}" href="/device">Préphériques</a>
            <a class="btn btn-theme {% if app.request.attributes.get('_route') =='os' %}disabled{% endif %}" href="/os">Systémes</a>
            <a class="btn btn-theme {% if app.request.attributes.get('_route') =='navigator' %}disabled{% endif %}" href="/navigator">Navigateur</a>
        </div>
        {% endif %}
    </div>
    <div class="clearfix"></div>
</div>